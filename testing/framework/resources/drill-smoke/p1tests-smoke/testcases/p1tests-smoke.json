{
    "testId": “p1tests-smoke”,
    "type": "group",
    "description": "Test p1tests smoke queries via jdbc",
    "categories": [
        "smoke"
    ],
    "matrices": [
        {
            "query-file": ".*.q",
            "schema": "dfs.drillTestDirP1",
            "output-format": "tsv",
            "expected-file": ".*.e_tsv",
            "verification-type": [
                "in-memory"
            ]
        }
    ],
    "datasources": [
        {
            "mode": "cp",
            "src": "p1tests/datasources/student.parquet",
            "dest": "p1tests/student/student.parquet"
        },
        {
            "mode": "cp",
            "src": "p1tests/datasources/voter.parquet",
            "dest": "p1tests/voter/voter.parquet"
        }
    ]
}
